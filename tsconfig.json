{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "newLine": "lf",
    "outDir": "lib",
    "moduleResolution": "node",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "removeComments": true
  },
  "exclude": ["**/*.test.ts", "/lib"],
  "include": ["src/**/*.ts"]
}
